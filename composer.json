{
  "name": "longdhdev/project_holaframework",
  "type": "project",
  "description": "A simple php framework",
  "homepage": "https://github.com/hailong289/hola-framework.git",
  "license": "MIT",
  "authors": [
    {
      "name": "hailong289",
      "email": "longdh.dev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "Queue\\Jobs\\": "queue/",
      "Request\\": "request/",
      "Commands\\": "commands/",
      "Mails\\": "mails/",
      "Middleware\\": "middleware/"
    },
    "classmap": [
      "App/",
      "queue/",
      "request/",
      "commands/",
      "mails/",
      "middleware/"
    ]
  },
  "config": {
    "platform-check": false
  },
  "require": {
    "php": ">=7.4",
    "longdhdev/holaframework": "v1.0.9"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "extra": {
    "branch-alias": {
      "dev-main": "v1.0.0-dev"
    }
  }
}
